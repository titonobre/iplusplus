{
  "name": "minesweeper-party",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "start-server": "node-dev server",
    "build": "npm-run-all build:*",
    "build:app": "ng build --prod --aot --build-optimizer --stats-json --source-map --extract-licenses",
    "build:server": "tsc -p server --outDir dist/server",
    "prebuild": "npm-run-all prepare:*",
    "prepare:generate-app-image": "svgexport svgexport.json",
    "prepare:minify-app-image": "imagemin src/assets/icons/*.png --out-dir=src/assets/icons",
    "serve": "node dist/server/server/server.js",
    "lint": "ng lint"
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/pwa": "0.13.5",
    "@angular/router": "8.2.14",
    "@angular/service-worker": "8.2.14",
    "compression": "1.7.3",
    "core-js": "2.6.5",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "pusher": "2.2.0",
    "pusher-js": "4.4.0",
    "rxjs": "6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.26",
    "@angular/cli": "8.3.26",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/node": "11.10.5",
    "codelyzer": "4.5.0",
    "imagemin": "6.1.0",
    "imagemin-cli": "4.0.0",
    "node-dev": "3.1.3",
    "npm-run-all": "4.1.5",
    "svgexport": "0.3.2",
    "ts-node": "8.0.3",
    "tslint": "5.13.1",
    "typescript": "3.5.3"
  },
  "heroku-run-build-script": true
}
