{
  "name": "minesweeper-party",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "start-server": "node-dev server",
    "build": "npm-run-all build:*",
    "build:app": "ng build --prod --aot --build-optimizer --stats-json --source-map --extract-licenses",
    "build:server": "tsc -p server --outDir dist/server",
    "postinstall": "npm run build",
    "prebuild": "npm-run-all prepare:*",
    "prepare:generate-app-image": "svgexport svgexport.json",
    "prepare:minify-app-image": "imagemin src/assets/icons/*.png --out-dir=src/assets/icons",
    "serve": "node dist/server/server/server.js",
    "lint": "ng lint"
  },
  "engines": {
    "node": "10.1.0",
    "npm": "6.0.0"
  },
  "dependencies": {
    "@angular/animations": "6.0.3",
    "@angular/cdk": "6.1.0",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/forms": "6.0.3",
    "@angular/http": "6.0.3",
    "@angular/material": "6.1.0",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/pwa": "0.6.3",
    "@angular/router": "6.0.3",
    "@angular/service-worker": "6.0.3",
    "compression": "1.7.2",
    "core-js": "2.5.6",
    "cors": "2.8.4",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "pusher": "2.0.0",
    "pusher-js": "4.2.2",
    "rxjs": "6.2.0",
    "rxjs-compat": "6.2.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.3",
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@types/node": "10.1.2",
    "codelyzer": "4.3.0",
    "imagemin": "5.3.1",
    "imagemin-cli": "3.0.0",
    "node-dev": "3.1.3",
    "npm-run-all": "4.1.3",
    "svgexport": "0.3.2",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.7.2"
  }
}
