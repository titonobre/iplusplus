{
  "name": "minesweeper-party",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "start-server": "node-dev server",
    "build": "npm-run-all build:*",
    "build:app": "ng build --prod --aot --build-optimizer --stats-json --source-map --extract-licenses",
    "build:server": "tsc -p server --outDir dist/server",
    "postinstall": "ngcc",
    "prebuild": "npm-run-all prepare:*",
    "prepare:generate-app-image": "svgexport svgexport.json",
    "prepare:minify-app-image": "imagemin src/assets/icons/*.png --out-dir=src/assets/icons",
    "serve": "node dist/server/server/server.js",
    "lint": "ng lint"
  },
  "engines": {
    "node": "12.16.3",
    "npm": "6.14.4"
  },
  "dependencies": {
    "@angular/animations": "9.1.6",
    "@angular/cdk": "9.2.3",
    "@angular/common": "9.1.6",
    "@angular/compiler": "9.1.6",
    "@angular/core": "9.1.6",
    "@angular/forms": "9.1.6",
    "@angular/material": "9.2.3",
    "@angular/platform-browser": "9.1.6",
    "@angular/platform-browser-dynamic": "9.1.6",
    "@angular/router": "9.1.6",
    "@angular/service-worker": "9.1.6",
    "compression": "1.7.4",
    "core-js": "2.6.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "pusher": "3.0.1",
    "pusher-js": "6.0.3",
    "rxjs": "6.6.0",
    "tslib": "1.13.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "9.1.4",
    "@angular/compiler-cli": "9.1.4",
    "@angular/language-service": "9.1.4",
    "@types/node": "12.11.1",
    "codelyzer": "5.2.2",
    "imagemin": "7.0.1",
    "imagemin-cli": "5.1.0",
    "node-dev": "3.1.3",
    "npm-run-all": "4.1.5",
    "svgexport": "0.4.0",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  },
  "heroku-run-build-script": true
}
